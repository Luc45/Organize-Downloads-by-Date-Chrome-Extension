{
    "name": "Organize Downloads by Date",
    "description": "Organizes your download folder by date. Example: YEAR/MONTH/FILE",
    "version": "1.0",
    "background": {
        "scripts": ["bg.js"],
        "persistent": false},
    "icons": {
        "16": "icons/16.png",
        "48": "icons/48.png",
        "128": "icons/128.png" },
    "permissions": ["downloads"],
    "content_security_policy": "script-src 'self'; default-src 'self'",
    "manifest_version": 2
}
